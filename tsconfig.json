{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": "/home/yuchiki/projects/astro-tutorial",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@scripts/*": [
        "src/scripts/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@typeDefinitions/*": [
        "src/types/*"
      ],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
